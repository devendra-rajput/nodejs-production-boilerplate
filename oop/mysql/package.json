{
  "name": "node-express-mysql-starter",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "dev:prod": "cross-env NODE_ENV=production node index.js",
    "db:push": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:push:prod": "cross-env NODE_ENV=production npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:drop:prod": "cross-env NODE_ENV=production npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:create:prod": "cross-env NODE_ENV=production npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:undo:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:prod": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": {
    "name": "Devendra Kumar",
    "url": "https://github.com/devendra-rajput"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "eslint-plugin-security": "^3.0.1",
    "express": "^5.2.1",
    "express-ejs-layouts": "^2.5.1",
    "heic-convert": "^2.1.0",
    "helmet": "^8.1.0",
    "i18n": "^0.15.3",
    "ioredis": "^5.9.2",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "minimist": "^1.2.8",
    "multer": "^2.0.2",
    "mysql2": "^3.16.1",
    "nodemailer": "^7.0.12",
    "nodemon": "^3.1.11",
    "rate-limiter-flexible": "^9.0.1",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "sequelize-cli": "^6.6.5",
    "socket.io-client": "^4.8.3"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^5.0.10",
    "inflight": "^1.0.6",
    "eslint-config-airbnb-base": {
      "eslint": "^9.0.0"
    }
  }
}